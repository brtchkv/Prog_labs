<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.AmbientLight?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shared.SkeletonRegister">
  <children>
    <MenuBar prefHeight="16.0" prefWidth="376.0" VBox.vgrow="ALWAYS">
      <menus>
            <Menu mnemonicParsing="false" text="Login">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#loginWindow" text="Login" />
               </items></Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#showHelp" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <BorderPane layoutX="3.0" prefHeight="379.0" prefWidth="363.0">
               <center>
                  <ImageView fitHeight="396.0" fitWidth="370.0" pickOnBounds="true" preserveRatio="true" x="232.0" y="22.0" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@new.png" />
                     </image>
                     <viewport>
                        <Rectangle2D />
                     </viewport>
                  </ImageView>
               </center></BorderPane>
            <Label alignment="CENTER" layoutX="147.0" layoutY="106.0" prefHeight="38.0" prefWidth="83.0" scaleX="2.0" scaleY="2.0" scaleZ="2.0" text="Register" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <effect>
                  <DropShadow blurType="GAUSSIAN" />
               </effect>
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="nick" layoutX="105.0" layoutY="163.0" promptText="Nickname">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></TextField>
            <TextField fx:id="email" layoutX="105.0" layoutY="209.0" promptText="Email" />
            <AmbientLight color="CHARTREUSE" lightOn="true" />
            <Button cancelButton="true" layoutX="106.0" layoutY="259.0" mnemonicParsing="false" onAction="#clear" text="Clear" textFill="#da1c1c" />
            <Button layoutX="210.0" layoutY="259.0" mnemonicParsing="false" onAction="#register" text="Submit" textFill="#367c05" />
      </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets />
         </VBox.margin>
    </AnchorPane>
  </children>
</VBox>
